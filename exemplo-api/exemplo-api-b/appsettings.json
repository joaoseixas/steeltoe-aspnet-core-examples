{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "api-b"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:9091/eureka/",
      "shouldFetchRegistry": true
    },
    "instance": {
      "hostName": "localhost",
      "port": 5002
    }
  },
  "management": {
    "endpoints": {
      "trace": {
        "enabled": true,
        "sensitive": true,
        "capacity": 100,
        "addRequestHeaders": true,
        "addResponseHeaders": true,
        "addPathInfo": true,
        "addUserPrincipal": true,
        "addParameters": true,
        "addQueryString": true,
        "addAuthType": true,
        "addRemoteAddress": true,
        "addSessionId": true,
        "addTimeTaken": false
      }
    }
  }
}